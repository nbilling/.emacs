(tool-bar-mode 0)
(ido-mode 1)
(set-cursor-color "orange")
(show-paren-mode 1)

(custom-set-variables
  ;; custom-set-variables was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(blink-cursor-mode nil)
 '(indent-tabs-mode nil)
 '(isar-tracing:auto-quickcheck nil)
 '(isar-tracing:auto-solve nil)
 '(isar-unicode-tokens-enable t)
 '(isar-x-symbol-enable nil)
 '(tool-bar-mode nil))
(custom-set-faces
  ;; custom-set-faces was added by Custom.
  ;; If you edit it by hand, you could mess it up, so be careful.
  ;; Your init file should contain only one such instance.
  ;; If there is more than one, they won't work right.
 '(proof-locked-face ((((type ns) (class color) (background light)) (:background "#98D9FA"))))
 '(proof-queue-face ((((type ns) (class color) (background light)) (:background "#E38F99"))))
 '(unicode-tokens-symbol-font-face ((t (:slant normal :weight normal :height 120 :width normal :foundry "apple" :family "Apple_Symbols")))))
